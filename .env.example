# Target OpenAI-compatible server URL
TARGET_SERVER_URL=https://your.target.server/v1

# API key for the target server
TARGET_API_KEY=your_target_api_key_here

# Fixed API key that clients must use to access this gateway
GATEWAY_API_KEY=your_gateway_api_key_here

# Port for the gateway to listen on
GATEWAY_PORT=8000

# Maximum number of log files to keep (older files will be deleted)
LOG_FILE_LIMIT=50
